{
    "name": "Amexio D3-chart Bar",
    "description": "An  D3-chart bar  chart that is rendered within the browser using SVG .Displays tips when hovering over points.",
    "selector": "<amexio-d3-chart-bar>",
    "properties": [
        {
            "name": "title",
            "description": "Sets title to chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        },

        {
            "name": "width",
            "description": "Width of chart",
            "version": "5.2 onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "height",
            "description": "Height of chart",
            "version": "5.2 onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "data",
            "description": "Local data for Chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        },
        {
            "name": "color",
            "description": "Sets color to D3 Bar chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        },
        {
            "name": "legend",
            "description": "Sets legend to D3 Bar chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        }
    ],
    "legend": [
        {
            "name": "onLegendClick",
            "description": "It will fire only on click on Legend Click in D3 bar chart ."
        }
    ]
}