{
    "name": "Amexio D3-chart donut",
    "description": "An   D3-chart donut chart that is rendered within the browser using SVG .Displays tips when hovering over points.",
    "selector": "<amexio-d3-chart-donut>",
    "properties": [
        {
            "name": "title",
            "description": "Sets title to chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        },
        {
            "name": "width",
            "description": "Width of chart",
            "version": "5.2 onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "height",
            "description": "Height of chart",
            "version": "5.2 onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "data",
            "description": "Local data for Chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        },
        {
            "name": "colors",
            "description": "Sets color to D3 donut chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        },
        {
            "name": "legend",
            "description": "Sets legend to D3 donut chart",
            "version": "5.2 onwards",
            "type": "any",
            "default": ""
        }
    ],
    "legend": [
        {
            "name": "onLegendClick",
            "description": "It will fire only on click on Legend Click in D3 donut chart ."
        }
    ]
}