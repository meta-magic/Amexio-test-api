    },
    {
      "name": "horizontalposition",
      "type": "string",
      "default": "null",
      "description": "Horizontal position of notification.Example right,left"
    }, {
      "name": "autodismissmsg",
      "type": "boolean",
      "default": "false",
      "description": "set true to Automatically dismiss notification "
    },
    {
      "name": "autodismissmsginterval",
      "type": "number",
      "default": "null",
      "description": "add time interval for shown msg "
    }
  ]
