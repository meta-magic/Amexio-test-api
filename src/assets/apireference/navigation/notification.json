{
  "properties": [
    {
      "name": "messageData",
      "type": "Array",
      "default": "null",
      "description": "list of notification with key msg."
    },
    {
      "name": "closable",
      "type": "boolean",
      "default": "true",
      "description": "closable icon on notification."
    },
    {
      "name": "verticalposition",
      "type": "string",
      "default": "null",
